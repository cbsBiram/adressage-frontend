{
  "expo": {
    "name": "MyHali",
    "slug": "myhali-app",
    "privacy": "public",
    "sdkVersion": "41.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "4.1.0",
    "orientation": "default",
    "icon": "./app/assets/logo.png",
    "splash": {
      "image": "./app/assets/MyHali-2.png",
      "resizeMode": "contain",
      "backgroundColor": "#f0f3f5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.origynslabs.myhali",
      "versionCode": 4
    },
    "description": "",
    "extra": {},
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "plugins": [
      "sentry-expo"
    ],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "your sentry organization's short name here",
            "project": "your sentry project's name here",
            "authToken": "your auth token here"
          }
        }
      ]
    }
  }
}


